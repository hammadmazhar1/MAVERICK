(1) =>(UserIsAway,Y(!(UserIsAway)))
(2) =>(DoorIsLocked,!(Y(Y(UserIsAway))))
(3) =>(TRUE,H(!(&(UserIsAway,Y(UserIsAway)))))
(4) =>(TRUE,!(Y(&(UserIsAway,Y(UserIsAway)))))
(5) =>(TRUE,S(!(UserIsAway),!(Y(UserIsAway))))
(6) =>(GasStoveIsOn,!(S(DoorIsLocked,Y(UserIsAway))))
(7) =>(|(BreadMakerIsOn,UserIsAway),Y(!(UserIsAway)))
(8) =>(=>(DoorIsLocked,UserIsAway),Y(!(UserIsAway)))
(9) =>(|(GasStoveIsOn,UserIsAway),Y(!(UserIsAway)))
(10) =>(TRUE,H(!(Y(&(UserIsAway,Y(UserIsAway))))))
